	<ViewingRule name="ACES 1.0 SDR-cinema (P3-clip)" viewTransform="ACES10_SDR-cinema_p3clip" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="SDR cinema" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 SDR-cinema" viewTransform="ACES10_SDR-cinema" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="SDR cinema" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 HDR-video 4000nits" viewTransform="ACES10_HDR-video_4000nits" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="HDR video" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 HDR-video 2000nits" viewTransform="ACES10_HDR-video_2000nits" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="HDR video" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 HDR-video 1000nits" viewTransform="ACES10_HDR-video_1000nits" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="HDR video" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Video (colorimetric)" viewTransform="colorimetric_view" allowedColorSpaces="Any Video" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Matte" viewTransform="matte_view" allowedColorSpaces="Any Data" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Normals" viewTransform="normals_view" allowedColorSpaces="Normals_201605" allowedCSContext="Specific" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
</viewingRules>

<?xml version="1.0" encoding="UTF-8"?>
<viewingRules version="1.0">
	<default viewTransform="Raw" />
	<ViewingRule name="Untagged warning" viewTransform="inverted-magenta_view" allowedColorSpaces="Unknown" allowedCSContext="Specific" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="Off" />
	<ViewingRule name="Log (low contrast)" viewTransform="log_raw_view" allowedColorSpaces="Any Log" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Linear (low contrast)" viewTransform="low-contrast_view" allowedColorSpaces="Any Scene-linear" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Log (gamma-corrected)" viewTransform="gamma-corrected_view" allowedColorSpaces="Any Log" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="Linear (gamma-corrected)" viewTransform="gamma-corrected_view" allowedColorSpaces="Any Scene-linear" allowedCSContext="Family" allowedDisplays="Any" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 SDR-video" viewTransform="ACES10_SDR-video" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="SDR video" allowedDisplaysContext="Family" isActive="On" />
	<ViewingRule name="ACES 1.0 SDR-video (D60 sim)" viewTransform="ACES10_SDR-video_d60sim" allowedColorSpaces="Any Scene-linear or Log" allowedCSContext="Family" allowedDisplays="SDR video" allowedDisplaysContext="Family" isActive="On" />

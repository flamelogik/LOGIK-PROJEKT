<?xml version="1.0" encoding="UTF-8"?>
<rules version="1.0">
	<default colorSpace="Rec.709 video"/>
	<FilePathRule name="OpenEXR" extension="[eE][xX][rR]" pattern="*" colorSpace="scene-linear Rec 709/sRGB" />
	<FilePathRule name="DPX" extension="[dD][pP][xX]" pattern="*" colorSpace="Log-to-Lin (jzp) / Rec.709" />
	<FilePathRule name="sRGB" extension="*" pattern="*[sS][rR][gG][bB]*" colorSpace="sRGB" />
	<FilePathRule name="Rec709" extension="*" pattern="*[rR][eE][cC]709*" colorSpace="Rec.709 video" />
	<FilePathRule name="matte" extension="*" pattern="*matte*" colorSpace="Matte_201605" />
	<FilePathRule name="alpha" extension="*" pattern="*alpha*" colorSpace="Matte_201605" />
</rules>
